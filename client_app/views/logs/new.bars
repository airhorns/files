<h1>New Log</h1>
{{#form_for "log"}}
  {{#field}}
    {{label "show_id"}}
    {{select "show_id" shows}}
  {{/field}}
  {{#field}}
    {{label "start_date"}}
    {{date "start_date"}}
    {{time "start_time"}}
  {{/field}}
  {{#field}}
    {{label "end_date"}}
    {{date "end_date"}}
    {{time "end_time"}}
  {{/field}}
  {{#field}}
    {{label "pre_recorded"}}
  {{/field}}

  <div class='actions'> 
    <input id="log_submit" name="commit" type="submit" value="Save" /> 
  </div> 
{{/form_for}}
