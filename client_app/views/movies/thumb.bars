  <div class="nail">
    {{#poster}}
    <img src="{{thumb.url}}" alt="Poster"/>
    {{^}}
    <img src="/images/fallback/poster_thumb_default.png" alt="Poster"/>
    {{/poster}}    
    <div class="actions" style="display: none;">
      <a href="#/movies/{{_id}}">Info</a>
      {{> movies/_shared_links}}
    </div>
  </div>
  <div class="title">
    <span class="title">{{title}}</span><span class="year">{{year}}</span>
  </div>
